#include "WPILib.h"
#include "Schematic.h"
#include "Thing.h"

class RobotDemo : public SimpleRobot
{
	Joystick Controller;
	 
	Thing SpdSet;
private:

public:
	RobotDemo(void):
		Controller(CONTROLLER)
	{
	
	} 
	void Autonomous()
	{
		
	}

	void OperatorControl()
	{
		while (IsOperatorControl())
		{
			SpdSet.Setspeed(Controller.GetRawAxis(2));
		}
	}
	
	void Test() {

	}
};

START_ROBOT_CLASS(RobotDemo);
