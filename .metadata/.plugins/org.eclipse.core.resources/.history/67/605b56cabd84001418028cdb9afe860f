#include "Thing.h"
#include "Schematic.h"
#include "WPILib.h"

void Thing::Setspeed(float axis)
{
	LDVictor.Set(axis);
	
	crate = LEnc.GetRate();
	if (crate >= max)
	{
		max = crate;
	}
	//Spd.SetPID(.01,0,0,.2)
	//Spd.SetSetpoint(axis);
	SmartDashboard::PutNumber("Left Rate", LEnc.GetRate());
	SmartDashboard::PutNumber("Max Left Rate", max);

}
