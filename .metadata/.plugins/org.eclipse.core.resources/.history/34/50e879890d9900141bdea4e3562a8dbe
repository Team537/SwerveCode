#ifndef MANUALDRIVE_H
#define MANUALDRIVE_H
#include "schematic.h"
#include <wpilib.h>

class ManualDrive
{
private:
	Victor SpeedOutput, AngleOutput;
public:
	ManualDrive(uint SpeedPort, uint AngPort):
		SpeedOutput(SpeedPort),
		AngleOutput(AngPort)
	{
	}
	void drive(float angle,float speed);
};
#endif
